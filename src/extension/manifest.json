{
  "name": "wdio-declarative-net-request",
  "description": "A Chrome extension utilizing the declarativeNetRequest API.",
  "version": "1.0.0",
  "manifest_version": 3,
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "ruleset_1",
        "enabled": true,
        "path": "rules.json"
      }
    ]
  },
  "permissions": [
    "declarativeNetRequest"
  ],
  "host_permissions": [
    "<all_urls>"
  ]
}